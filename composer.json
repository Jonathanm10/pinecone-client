{
    "name": "jonathanm10/pinecone-client",
    "description": "A simple wrapper around Pinecone API",
    "keywords": [
        "Jonathanm10",
        "pinecone-client"
    ],
    "homepage": "https://github.com/jonathanm10/pinecone-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Macheret",
            "email": "jonathan.macheret@liip.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Jonathanm10\\PineconeClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonathanm10\\PineconeClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
